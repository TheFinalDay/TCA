{
  "expo": {
    "name": "TCA",
    "slug": "tourney-companion-app",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-mascot.png",
    "splash": {
      "image": "./assets/splash-mascot.png",
      "resizeMode": "contain",
      "backgroundColor": "#4734b3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "useNextNotificationsApi": true,
      "icon": "./assets/icon-mascot.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-mascot.png",
        "backgroundColor": "#4734b3"
      }
    },
    "description": "A quality of life & tournament management app for players and TOs respectively",
    "githubUrl": "https://github.com/TheFinalDay/TCA"
  }
}
